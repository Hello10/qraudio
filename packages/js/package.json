{
  "name": "qraudio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "default": "./dist/node/index.js"
    },
    "./web": {
      "types": "./dist/web/index.d.ts",
      "default": "./dist/web/index.js"
    }
  },
  "bin": {
    "qraudio": "./dist/node/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch --preserveWatchOutput",
    "typecheck": "tsc -b",
    "lint": "biome check .",
    "format": "biome format --write .",
    "fix": "biome check --write .",
    "pretest": "tsc -p tsconfig.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest -c jest.config.mjs",
    "test:integration": "NODE_OPTIONS=--experimental-vm-modules jest -c jest.integration.config.mjs"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  }
}
